<dotnetnuke version="3.0" type="Module">
  <folders>
    <folder>
      <name>TweetDNN</name>
      <friendlyname>TweetDNN</friendlyname>
      <foldername>TweetDNN</foldername>
      <modulename>TweetDNN</modulename>
      <description>TweetDNN is a simple Twitter feed module for DNN that supports CSS styling. All of the options are located on the settings page. Please tweet me @brillsteve with any questions/comments.</description>
      <version>01.00.01</version>
      <businesscontrollerclass>
      </businesscontrollerclass>
      <modules>
        <module>
          <friendlyname>TweetDNN</friendlyname>
          <cachetime>0</cachetime>
          <controls>
            <control>
              <title>TweetDNN</title>
              <src>DesktopModules/TweetDNN/View.ascx</src>
              <type>View</type>
            </control>
            <control>
              <key>Settings</key>
              <title>TweetDNN</title>
              <src>DesktopModules/TweetDNN/Settings.ascx</src>
              <type>Edit</type>
            </control>
          </controls>
        </module>
      </modules>
      <files>
        <file>
          <name>Settings.ascx</name>
        </file>
        <file>
          <name>Settings.ascx.vb</name>
        </file>
        <file>
          <name>TweetDNN.css</name>
        </file>
        <file>
          <name>View.ascx</name>
        </file>
        <file>
          <name>View.ascx.vb</name>
        </file>
        <file>
          <path>[app_code]</path>
          <name>Status.vb</name>
        </file>
        <file>
          <path>[app_code]</path>
          <name>Twitter.vb</name>
        </file>
      </files>
    </folder>
  </folders>
</dotnetnuke>